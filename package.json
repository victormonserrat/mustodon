{
  "name": "@victormonserrat/mustodon",
  "version": "0.0.1",
  "description": "A to do list example DDD + CQRS + ES application.",
  "author": "Victor Monserrat",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/src/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/src/main.js\"",
    "start:prod": "node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/cqrs": "^6.0.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/swagger": "^3.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "dotenv": "^8.1.0",
    "geteventstore-promise": "^3.1.2",
    "mongoose": "^5.6.11",
    "randexp": "^0.5.3",
    "reflect-metadata": "0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.5.2",
    "shallow-equal-object": "^1.1.1",
    "swagger-ui-express": "^4.0.7",
    "yarn": "^1.22.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.6.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/mongoose": "^5.5.16",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "3.4.3"
  }
}
